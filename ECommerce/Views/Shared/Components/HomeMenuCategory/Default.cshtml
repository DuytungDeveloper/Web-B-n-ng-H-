@model List<ECommerce.Model.EFModel.Models.Category>
@using ECommerce.Models.View
@for (int i = 0; i < Model.Count(); i++)
{
    <li>
        @Html.ActionLink(
            linkText: Model[i].Name,
            actionName: "Index",
            controllerName: "Search-Result",
            routeValues: new SearchViewModel
            {
                CategoryId = new List<int>() { Model[i].Id }
            },
            htmlAttributes: null
        )
    </li>
}
